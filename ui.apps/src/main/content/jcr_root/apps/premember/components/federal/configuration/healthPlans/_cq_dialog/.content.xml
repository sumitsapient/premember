<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Plan Type"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <labels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Plan Comparison Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <planCosts
                                        jcr:primaryType="nt:unstructured"
                                        margin="{Boolean}true"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Plan Costs"/>
                                            <well
                                                granite:class="cmp-healthPlan__editor-row"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <planCostsTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        maxlength="255"
                                                        name="./labels/planCostsTitle"
                                                        value="Health Plant Costs"/>
                                                    <planCostsSubtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Subtitle"
                                                        maxlength="255"
                                                        name="./labels/planCostsSubtitle"
                                                        value="Amounts You Pay"/>
                                                    <planCostsDeductibleTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deductible"
                                                        maxlength="255"
                                                        name="./labels/planCostsDeductibleTitle"
                                                        value="Deductible"/>
                                                    <planCostsDeductibleDescription
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deductible Description"
                                                        name="./labels/planCostsDeductibleDescription"
                                                        value="Amount you pay before your plan starts sharing costs."/>
                                                    <planCostsOutOfPocketLimitTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Out-of-pocket Limit"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/planCostsOutOfPocketLimitTitle"
                                                        value="Out-of-pocket Limit"
                                                        valueType="string"/>
                                                    <planCostsOutOfPocketLimitDescription
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Out-of-pocket Limit Description"
                                                        name="./labels/planCostsOutOfPocketLimitDescription"
                                                        value="Amount you pay before your plan covers all costs."/>
                                                </items>
                                            </well>
                                        </items>
                                    </planCosts>
                                    <medicalBenefits
                                        jcr:primaryType="nt:unstructured"
                                        margin="{Boolean}true"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Medical Benefits"/>
                                             <well
                                                granite:class="cmp-healthPlan__editor-row"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <medicalBenefitsTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsTitle"
                                                        value="Medical Benefits"
                                                        valueType="string"/>
                                                    <medicalBenefitsSubtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Subtitle"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsSubtitle"
                                                        value="What You Pay"
                                                        valueType="string"/>
                                                    <medicalBenefitsDeductible
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deductible"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsDeductible"
                                                        value="Deductible"
                                                        valueType="string"/>
                                                    <medicalBenefitsOutOfPocketMaximum
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Out-of-Pocket Maximum"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsOutOfPocketMaximum"
                                                        value="Out-of-Pocket Maximum"
                                                        valueType="string"/>
                                                    <medicalBenefitsPreventiveCare
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Preventive Care"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsPreventiveCare"
                                                        value="Preventive Care"
                                                        valueType="string"/>
                                                    <medicalBenefitsMedicalCare
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Medical Care"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsMedicalCare"
                                                        value="Medical Care"
                                                        valueType="string"/>
                                                    <medicalBenefitsHospitalization
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Hospitalization"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsHospitalization"
                                                        value="Hospitalization"
                                                        valueType="string"/>
                                                    <medicalBenefitsOutpatientSurgery
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Outpatient Surgery"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/medicalBenefitsOutpatientSurgery"
                                                        value="Outpatient Surgery"
                                                        valueType="string"/>
                                                </items>
                                             </well>
                                        </items>
                                    </medicalBenefits>
                                    <morePlanInfo
                                        jcr:primaryType="nt:unstructured"
                                        margin="{Boolean}true"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="More Plan Information"/>
                                            <well
                                                granite:class="cmp-healthPlan__editor-row"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <meorePlanInfoTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/morePlanInfoTitle"
                                                        value="More Plan Information"
                                                        valueType="string"/>
                                                    <morePlanInfoBenefits
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Benefits"
                                                        maxlength="255"
                                                        metaType="text-single"
                                                        name="./labels/morePlanInfoBenefits"
                                                        value="More benefits information"
                                                        valueType="string"/>
                                                </items>
                                            </well>
                                        </items>
                                    </morePlanInfo>
                                </items>
                            </columns>
                        </items>
                    </labels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
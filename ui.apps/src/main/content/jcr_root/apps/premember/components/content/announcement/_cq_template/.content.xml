<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          title="Lorem Ipsum"
          titleType="h4"
          hideImage="{Boolean}false"
          leftAlignImage="{Boolean}false">
    <image
            jcr:primaryType="nt:unstructured"
            sling:resourceType="premember/components/content/image"/>
    <text
            jcr:primaryType="nt:unstructured"
            sling:resourceType="premember/components/content/text"
            text="&lt;p&gt;Lorem ipsum dolor sit amet.&lt;/p&gt;&#xa;"
            textIsRich="true"/>
    <button
            jcr:primaryType="nt:unstructured"
            sling:resourceType="premember/components/content/button"
            text="Primary"
            naked="{Boolean}true"
    />
    <pdfButton
            jcr:primaryType="nt:unstructured"
            sling:resourceType="premember/components/content/button"
            text="Primary"
            naked="{Boolean}true"
    />
</jcr:root>

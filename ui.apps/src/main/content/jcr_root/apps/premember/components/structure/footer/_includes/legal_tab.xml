<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="nt:unstructured"
  jcr:title="Legal"
  sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
  <items jcr:primaryType="nt:unstructured">
    <column
      jcr:primaryType="nt:unstructured"
      sling:resourceType="granite/ui/components/coral/foundation/container"
      margin="{Boolean}true">
      <items jcr:primaryType="nt:unstructured">
        <projectId
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
          fieldLabel="Project ID"
          required="{Boolean}false"
          name="./projectId"/>
        <assetNumber
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
          fieldLabel="Asset Number"
          required="{Boolean}false"
          name="./assetNumber"/>
        <copyrightInformation
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
          fieldLabel="Copyright Information"
          fieldDescription="\{dateYear\} is used for dynamically year update"
          required="{Boolean}true"
          name="./copyrightInformation"/>
        <oxfordCopyrightInformation
          jcr:primaryType="nt:unstructured"
          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
          fieldLabel="Oxford Copyright Information"
          fieldDescription="\{dateYear\} is used for dynamically year update"
          required="{Boolean}true"
          name="./oxfordCopyrightInformation"/>
      </items>
    </column>
  </items>
</jcr:root>

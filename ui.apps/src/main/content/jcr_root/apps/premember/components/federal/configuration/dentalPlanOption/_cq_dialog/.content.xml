<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Federal - Dental Plan"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[premember.configuration.federa.planOption.dialog]">
    <content
        granite:class="cmp-dentalPlan__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <granite:data
            jcr:primaryType="nt:unstructured"
            component-path="${requestPathInfo.suffix}"/>
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
					<general
						granite:class="generalEmployerPlanTab"
						jcr:primaryType="nt:unstructured"
						jcr:title="General"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<id
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/select"
								emptyOption="{Boolean}true"
								fieldLabel="Name"
								name="./id"
								required="{Boolean}true"
								valueType="string">
								<datasource
									jcr:primaryType="nt:unstructured"
									sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
									path="/etc/acs-commons/lists/federal/plan-names/dental-plan-names"/>
							</id>
						</items>
					</general>
                    <planCosts
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Plan Costs"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
							<deductibleIndividual
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
								fieldLabel="Deductible - In Network ($)"
								metaType="number"
								min="0"
								name="./planCosts/deductibleIndividual"
								required="{Boolean}true"
								step="any"
								typeHint="long"
								valueType="long"/>
							<deductibleFamily
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
								fieldLabel="Deductible - Out of Network ($)"
								metaType="number"
								min="0"
								name="./planCosts/deductibleFamily"
								required="{Boolean}true"
								step="any"
								typeHint="long"
								valueType="long"/>
							<annualMaximumIndividual
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Annual Maximum - In Network"
								name="./planCosts/annualMaximumIndividual"
								required="{Boolean}true"/>
							<annualMaximumFamily
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Annual Maximum - Out of Network"
								name="./planCosts/annualMaximumFamily"
								required="{Boolean}true"/>
						</items>
					</planCosts>
					<deductibles
						jcr:primaryType="nt:unstructured"
						jcr:title="Deductibles"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<inDeductibles
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<heading
										granite:class="coral-Heading coral-Heading--4"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/heading"
										level="{Long}4"
										text="In Network"/>
									<well
									granite:class="cmp-dentalPlan__editor-row"
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/well">
									<items jcr:primaryType="nt:unstructured">
										<inDeductible
											granite:class="cmp-healthPlan__editor-Field"
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
											fieldLabel="Deductible"
											metaType="number"
											min="0"
											name="./deductibles/inDeductible"
											step="any"
											typeHint="long"
											valueType="long"/>
									</items>
									</well>
								</items>
							</inDeductibles>
							<oonDeductibles
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<heading
										granite:class="coral-Heading coral-Heading--4"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/heading"
										level="{Long}4"
										text="Out of Network"/>
									<well
										granite:class="cmp-dentalPlan__editor-row"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/well">
										<items jcr:primaryType="nt:unstructured">
											<oonDeductibleSelfOnly
												granite:class="cmp-healthPlan__editor-Field"
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
												fieldLabel="Self Only ($)"
												metaType="number"
												min="0"
												name="./deductibles/oonSelfOnly"
												step="any"
												typeHint="long"
												valueType="long"/>
											<oonDeductibleSelfPlusOne
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
												fieldLabel="Self Plus One ($)"
												metaType="number"
												min="0"
												name="./deductibles/oonSelfPlusOne"
												step="any"
												typeHint="long"
												valueType="long"/>
											<oonDeductibleSelfPlusFamily
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
												fieldLabel="Self Plus Family ($)"
												metaType="number"
												min="0"
												name="./deductibles/oonSelfPlusFamily"
												step="any"
												typeHint="long"
												valueType="long"/>
										</items>
									</well>
								</items>
							</oonDeductibles>
						</items>
					</deductibles>
					<preventiveCareTab
						jcr:primaryType="nt:unstructured"
						jcr:title="Preventive Care"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<preventiveCare
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<heading
										granite:class="coral-Heading coral-Heading--4"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/heading"
										level="{Long}4"
										text="Preventive Care"/>
									<well
										granite:class="cmp-dentalPlan__editor-row"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/well">
										<items jcr:primaryType="nt:unstructured">
											<inRow jcr:primaryType="nt:unstructured"
												   granite:class="cmp-employerPlanOption__generalAmount-editor-row "
												   sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
													<inPreventiveCare
														granite:class="cmp-healthPlan__editor-Field"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
														fieldLabel="In Network"
														metaType="number"
														min="0"
														name="./preventiveCare/inPreventiveCare"
														step="any"
														typeHint="long"
														valueType="long"/>
													<inPreventiveCareIsPercentage
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
														name="./preventiveCare/inPreventiveCareIsPercentage"
														text="%"
														value="{Boolean}true"
														uncheckedValue="{Boolean}false"/>
												</items>
											</inRow>
											<ooRow jcr:primaryType="nt:unstructured"

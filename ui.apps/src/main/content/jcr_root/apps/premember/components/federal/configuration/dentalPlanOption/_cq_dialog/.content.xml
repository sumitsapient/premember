									sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
								<waitingPeriodsTitle
									granite:class="coral-Heading coral-Heading--4"
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/heading"
									level="{Long}4"
									text="Waiting Periods"/>
								<well
									granite:class="cmp-healthPlan__editor-row"
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/well">
									<items jcr:primaryType="nt:unstructured">
										<waitingPeriodsClassA
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container">
											<items jcr:primaryType="nt:unstructured">
												<heading
													granite:class="coral-Heading coral-Heading--4"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/heading"
													level="{Long}4"
													text="Class A"/>
												<well
													granite:class="cmp-healthPlan__editor-row"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/well">
													<items jcr:primaryType="nt:unstructured">
														<inWaitingPeriodClassA
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/inWaitingPeriodClassA"
															text="In Network"
															uncheckedValue="false"/>
														<oonWaitingPeriodClassA
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/oonWaitingPeriodClassA"
															text="Out of Network"
															uncheckedValue="false"/>
													</items>
												</well>
											</items>
										</waitingPeriodsClassA>
										<waitingPeriodsClassB
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container">
											<items jcr:primaryType="nt:unstructured">
												<heading
													granite:class="coral-Heading coral-Heading--4"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/heading"
													level="{Long}4"
													text="Class B"/>
												<well
													granite:class="cmp-healthPlan__editor-row"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/well">
													<items jcr:primaryType="nt:unstructured">
														<inWaitingPeriodClassB
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/inWaitingPeriodClassB"
															text="In Network"
															uncheckedValue="false"/>
														<oonWaitingPeriodClassB
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/oonWaitingPeriodClassB"
															text="Out of Network"
															uncheckedValue="false"/>
													</items>
												</well>
											</items>
										</waitingPeriodsClassB>
										<waitingPeriodsClassC
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container">
											<items jcr:primaryType="nt:unstructured">
												<heading
													granite:class="coral-Heading coral-Heading--4"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/heading"
													level="{Long}4"
													text="Class C"/>
												<well
													granite:class="cmp-healthPlan__editor-row"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/well">
													<items jcr:primaryType="nt:unstructured">
														<inWaitingPeriodClassC
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/inWaitingPeriodClassC"
															text="In Network"
															uncheckedValue="false"/>
														<oonWaitingPeriodClassC
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/oonWaitingPeriodClassC"
															text="Out of Network"
															uncheckedValue="false"/>
													</items>
												</well>
											</items>
										</waitingPeriodsClassC>
										<waitingPeriodsClassD
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container">
											<items jcr:primaryType="nt:unstructured">
												<heading
													granite:class="coral-Heading coral-Heading--4"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/heading"
													level="{Long}4"
													text="Class D"/>
												<well
													granite:class="cmp-healthPlan__editor-row"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/well">
													<items jcr:primaryType="nt:unstructured">
														<inWaitingPeriodClassD
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/inWaitingPeriodClassD"
															text="In Network"
															uncheckedValue="false"/>
														<oonWaitingPeriodClassD
															granite:class="cmp-healthPlan__editor-Field"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
															value="true"
															name="./preventiveCare/oonWaitingPeriodClassD"
															text="Out of Network"
															uncheckedValue="false"/>
													</items>
												</well>
											</items>
										</waitingPeriodsClassD>
									</items>
								</well>
								</items>
							</waitingPeriods>
						</items>
					</preventiveCareTab>
					<medicalCareTab
						jcr:primaryType="nt:unstructured"
						jcr:title="Medical Care"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<diagnosticServices
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<heading
										granite:class="coral-Heading coral-Heading--4"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/heading"
										level="{Long}4"
										text="Diagnostic Services ($)"/>
									<well
										granite:class="cmp-dentalPlan__editor-row"
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/well">
										<items jcr:primaryType="nt:unstructured">
											<inDiagnosticServices
												granite:class="cmp-healthPlan__editor-Field"
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
												fieldLabel="In Network"
												metaType="number"
												min="0"
												name="./medicalCare/inDiagnosticServices"
												step="any"
												typeHint="long"
												valueType="long"/>
											<inDiagnosticServicesIsPercentage
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
													name="./medicalCare/inDiagnosticServicesIsPercentage"
													text="%"
													value="{Boolean}true"
													uncheckedValue="{Boolean}false"/>
											<oonDiagnosticServices
												granite:class="cmp-healthPlan__editor-Field"
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
												fieldLabel="Out Of Network"
												metaType="number"
												min="0"
												name="./medicalCare/oonDiagnosticServices"
												step="any"
												typeHint="long"

												   sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
													<oonPreventiveCare
														granite:class="cmp-healthPlan__editor-Field"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
														fieldLabel="Out Of Network"
														metaType="number"
														min="0"
														name="./preventiveCare/oonDiagnosticServices"
														step="any"
														typeHint="long"
														valueType="long"/>
													<oonPreventiveCareIsPercentage
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
														name="./preventiveCare/oonDiagnosticServicesIsPercentage"
														text="%"
														value="{Boolean}true"
														uncheckedValue="{Boolean}false"/>
												</items>
											</ooRow>
										</items>
									</well>
								</items>
							</preventiveCare>
							<waitingPeriods

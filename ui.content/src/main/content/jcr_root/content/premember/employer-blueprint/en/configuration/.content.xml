<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/premember/settings/wcm/templates/employer-config-page-template"
        jcr:primaryType="cq:PageContent"
        jcr:title="Configuration"
        sling:resourceType="premember/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <employerData
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/employerData"/>
                <healthPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/healthPlans"
                    planType="health"/>
                <visionPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/visionPlans"
                    planType="vision"/>
                <dentalPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/dentalPlans"
                    planType="dental"/>
                <pharmacyPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/pharmacyPlans"
                    planType="pharmacy"/>
                <fsaPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/fsaPlans"
                    planType="fsa"/>
                <financialPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/financialPlans"
                    planType="financial"/>
                <behavioralhealthPlans
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/behavioralhealthPlans"
                    planType="behavioralhealth"/>
                <footerConfig
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="premember/components/configuration/footerConfig"
                    customerServicePhone="1-877-836-3236"
                    displayMemberSupport="{Boolean}true"
                    operationHours="8 a.m. - 6 p.m. ET"/>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>

                                                sling:resourceType="premember/components/structure/layout/column">
                                                <text
                                                    cq:styleIds="[1544131899790]"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="premember/components/content/text"
                                                    text="&lt;p>&lt;b>Dental&lt;/b>&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>Artificial teeth&lt;/li>&#xd;&#xa;&lt;li>Braces&lt;/li>&#xd;&#xa;&lt;li>Dental services such as exams, cleanings, fillings, and X-rays&lt;/li>&#xd;&#xa;&lt;li>Dental plan deductible, coinsurance and copayments&lt;/li>&#xd;&#xa;&lt;li>Mouth guards&lt;/li>&#xd;&#xa;&lt;li>Health plan deductible, coinsurance and copayments —if dental services are covered&lt;/li>&#xd;&#xa;&lt;li>Orthodontia services&lt;/li>&#xd;&#xa;&lt;li>Tooth caps and tooth removals&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"

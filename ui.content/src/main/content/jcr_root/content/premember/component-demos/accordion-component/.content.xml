                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="premember/components/content/text"
                                        text="&lt;p>Hospital indemnity insurance can work alongside your health plan by providing cash payments to help protect your budget from the high costs of hospital bills.&amp;nbsp; Hospital Indemnity insurance pays you cash after hospital care. You can use the cash any way you choose such as paying expenses from a hospital stay or related treatment, your health care deductible, and other out-of-pocket costs.&lt;/p>&#xa;"
                                        textIsRich="true"/>
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="premember/components/content/accordionGroup"
                                        expandAsDefault="false"
                                        mobileOnly="true"
                                        titleType="h3">
                                        <accordioncmp0
                                            jcr:primaryType="nt:unstructured"
